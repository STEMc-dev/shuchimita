{
	"version": 2,
	"name": "shuchimitav2",
	"builds": [{ "src": "index.js", "use": "@vercel/node" }],
	"routes": [{ "src": "/(.*)", "dest": "/index.js" }],

	"rewrites": [
    {
      "source": "/registration",
      "missing": [
        {
          "type": "cookie",
          "key": "auth_token"
        }
      ],
      "destination": "/login"
    }
  ]

}